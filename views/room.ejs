<!doctype html>
<html lang='en'>
	<head>
		<title>Face Express</title>
		
		<link rel='stylesheet' href='/stylesheets/style.css'></link>
		
		
		<link rel='stylesheet' href='/stylesheets/demo.css'></link>
		<link rel='stylesheet' href='/stylesheets/bootstrap.css'></link>
		
		<link rel='stylesheet' href='/stylesheets/flat-ui.css'></link>
		<link rel="stylesheet" class="umbrui" href="elements/range.css"/>
		<link rel="stylesheet" class="umbrui" href="elements/checkbox.css"/>
		<link rel="stylesheet" class="umbrui" href="elements/progress.css"/>
		<link rel="stylesheet" href="/stylesheets/nanoscroller.css"></link>
		<link rel="stylesheet" href="/stylesheets/nanostyle.css"></link>
	</head>
	<body>
		<div class="container">
			<div class="row">
				<div class="col-md-8 .col-xs-8">
					 <div class='row video-box'>
						<!-- Video -->
						<div class='inner-video-box'>
							<video autoplay class="main-video remote-video" id='main-video' src="" ></video>
						</div>
					</div>


					<div class='row control-bar'>
						<!-- Video Controls -->
						<div  class='col-md-1 .col-xs-1'>
							<button  class="btn btn-embossed btn-success btn-sm" id="play-pause">
								<span class="glyphicon glyphicon-play"></span>
							</button>
						</div>
						
						
						<div class='col-md-6 .col-xs-6' >
						<!--	<div class='my-slider'></div>
							<div id="slider" class="ui-slider"></div>-->
						</div>
						<div class='col-md-1 .col-xs-1'>
							<button  class="btn btn-embossed btn-inverse btn-sm" id="volume-down">
								<span class="glyphicon glyphicon-volume-down"></span>
							</button>
						</div>
						
						<div class='col-md-1 .col-xs-1'>
							<button  class="btn btn-embossed btn-inverse btn-sm" id="volume-up">
								<span class="glyphicon glyphicon-volume-up"></span>
							</button>
						</div>
						<div class='col-md-1 .col-xs-1'>
								<span></span>
						</div>
						<div class='col-md-2 .col-xs-2'>
							<button  class="btn btn-embossed btn-inverse btn-sm" id="full-screen" >
							<span class="glyphicon glyphicon-fullscreen"></span></button>
						</div>
					</div>
					<div class="row">
						<div class="horizontal-bar">
							<div class='col-md-4 .col-xs-4'>
								<video autoplay class="mini-video remote-video first-video" id="v1"></video>
							</div>
							<div class='col-md-4 .col-xs-4'>
								<video autoplay class="mini-video remote-video" id="v2"></video>
							</div>
							<div class='col-md-4 .col-xs-4'>
								<video autoplay class="mini-video remote-video last-video" id="v3" ></video>
							</div>
						</div>
					</div>
				</div>
				<div class="col-md-4 .col-xs-4">
					<div class='row '>
						<div class='col-md-2 .col-xs-2'></div>
						<div class='col-md-6 .col-xs-2 localvideo-box'><div class='inner-box'><video id='localvideo' muted="muted" ></video></div></div>
					</div>
					<div class="row chatbox-header">
					
					</div>
					<div class="row chatbox-border" id="chat-box">
						<div class="chatbox-content nano" >
							<div id='message-box' class='content'>
								<!--<div class="message-others" >
									<div class="bubble-other"><small>Welcome to Face Express</small></div>
								</div>-->
								
							</div>
						</div>
					</div>
					<div class="row chatbox-bar">
						
						<div class="col-md-8 .col-xs-8 has-success">
							<input type="text" placeholder="Enter" class="input-sm flat input-box " id="input-box">
						</div>
						<div class="col-md-2 .col-xs-2">
							<button class="btn btn-embossed btn-success btn-sm" id='send'>发送</button>
						</div>
					</div>
				</div>

			</div>
			
<!-- Modal -->
<div class="modal fade" id="myModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <h6 class="modal-title" id="myModalLabel">请输入您的<strong>昵称</strong></h6>
      </div>
      <div class="modal-body">
        <input type="text" placeholder="昵称" class="input-sm flat nickname " id="nickname"> 
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-embossed btn-success btn-sm" id='ok'>确定</button>
        
      </div>
    </div><!-- /.modal-content -->
  </div><!-- /.modal-dialog -->
</div><!-- /.modal -->

	<aside id="settings">
      <div id="settings_icon" class="inactive glyphicon glyphicon-chevron-right"></div>
      <div id="settings_pannel">
        <ul>
          <li><span>本地视频</span><input id="local" type="checkbox" class="display_blocks" data-icon1="关" data-icon2="开" checked=false></li>
          <li><span>候选视频 1</span><input id="remote1" type="checkbox" class="display_blocks" data-icon1="关" data-icon2="开" checked=false></li>
          <li><span>候选视频 2</span><input id="remote2" type="checkbox" class="display_blocks" data-icon1="关" data-icon2="开" checked=false></li>
          <li><span>候选视频 3</span><input id="remote3" type="checkbox" class="display_blocks" data-icon1="关" data-icon2="开" checked=false></li>
          <li><span>主视频音量</span><input  class="display_blocks range" type="range" min="0" max="100" value="100"></li>
          <li><span>候选视频音量 1</span><input class="display_blocks range" type="range" min="0" max="100" value="100"></li>
          <li><span>候选视频音量 2 </span><input  class="display_blocks range" type="range" min="0" max="100" value="100"></li>
          <li><span>候选视频音量 3</span><input class="display_blocks range" type="range" min="0" max="100" value="100"></li>
          
          <!--<li><span>Background</span></li>-->
        </ul>
        <!--<div id="bg-blocks">
            <section class="background-selected selected"></section>
            <section class="background-selected"></section>
            <section class="background-selected"></section>
            <section class="background-selected"></section>
            <section class="background-selected"></section>
        </div>-->
      </div>
    </aside>
</div>
		<script src="/socket.io/socket.io.js" type='text/javascript'></script>
		<script src="/javascripts/jquery-1.8.3.min.js" type='text/javascript'></script> 
        <script src="/javascripts/jquery-ui-1.10.3.custom.min.js"></script>
        <script src='/javascripts/bootstrap.min.js' type='text/javascript'></script>
        
        <script src="/javascripts/nanoscroller.js"></script>
        
        <script src='/javascripts/application.js' type='text/javascript'></script>
        <script src="/javascripts/webrtc.js" type='text/javascript'></script>
        <script src="/javascripts/file-share.js" type="text/javascript"></script>
        <script src="/javascripts/room.js" type='text/javascript'></script>
        

    </body>
</html>
<script>
	
	$('#settings_icon').click(function(){
		
		if(!!$('#settings_icon').hasClass('inactive')){
			$('#settings_icon').removeClass('inactive').addClass('active');
			
			$('#settings').animate({
				left:"0px"
			},600);
			setTimeout(function () {
			   	$('#settings_icon').removeClass('glyphicon-chevron-right').addClass('glyphicon-chevron-left');
			}, 600);

			 
		}else{
			$('#settings_icon').removeClass('active').addClass('inactive');
			
			$('#settings').animate({
				left:"-400px"
			},600);
			setTimeout(function () {
			   	$('#settings_icon').removeClass('glyphicon-chevron-left').addClass('glyphicon-chevron-right');
			}, 600);
			
		}
	});
	$('#local').click(function(){
		if(document.getElementById('local').checked)
			document.getElementById('localvideo').play();
		else
			document.getElementById('localvideo').pause();
	})
	$('#remote1').click(function(){
		if(document.getElementById('remote1').checked)
			document.getElementById('v1').play();
		else
			document.getElementById('v1').pause();
	})
	$('#remote2').click(function(){
		if(document.getElementById('remote2').checked)
			document.getElementById('v2').play();
		else
			document.getElementById('v2').pause();
	})
	$('#remote3').click(function(){
		if(document.getElementById('remote3').checked)
			document.getElementById('v3').play();
		else
			document.getElementById('v3').pause();
	})

</script>

